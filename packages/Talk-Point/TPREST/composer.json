{
    "name": "Talk-Point/TPREST",
    "type": "library",
    "description": "Package for Laravel to create REST API",
    "keywords": [
        "Talk-Point",
        "TPREST"
    ],
    "homepage": "https://github.com/Talk-Point/TPREST",
    "license": "MIT",
    "authors": [
        {
            "name": "Talk-Point",
            "email": "it@talk-point.de",
            "homepage": "https://github.com/Talk-Point",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "TPREST\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TPREST\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
